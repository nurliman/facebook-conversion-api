{
  "name": "@nurliman/facebook-conversion-api",
  "version": "1.2.3",
  "description": "Facebook Conversion API Wrapper for Node.js",
  "license": "ISC",
  "author": {
    "name": "Nurliman Diara",
    "email": "nurlimandiara@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nurliman/facebook-conversion-api.git"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:common && npm run build:types",
    "build:common": "node scripts/build.mjs --minify",
    "build:types": "tsc --declaration --allowJs --emitDeclarationOnly",
    "format": "pretty-quick --staged --ignore-path .gitignore",
    "format:all": "prettier --write \"**/*.+(js|mjs|cjs|jsx|ts|tsx|css|scss|md|json|yml|yaml)\" --ignore-path .gitignore",
    "prepublish": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "esbuild": "^0.14.47",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.16.0",
    "globby": "^13.1.1",
    "has-flag": "^5.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "facebook-nodejs-business-sdk": "^13.0.0"
  },
  "lint-staged": {
    "*.+(js|mjs|cjs|jsx|ts|tsx)": "eslint --cache --fix"
  }
}
